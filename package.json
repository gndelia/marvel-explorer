{
  "name": "marvel-explorer",
  "description": "Explore your favourite characters from the Marvel Universe!",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "awesome-debounce-promise": "^2.1.0",
    "gh-pages": "^2.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src/**/*.js",
    "test": "react-scripts test",
    "test:coverage": "cross-env CI=true react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/Api/Fetch.js",
      "!**/serviceWorker.js",
      "!**/index.js",
      "!**/setupTests.js",
      "!**/Store/ConfigureStore.js",
      "!**/Store/ReducersConfiguration.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.3"
  },
  "homepage": "",
  "author": "Gonzalo D'Elia",
  "bugs": {
    "url": "https://github.com/gndelia/marvel-explorer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gndelia/marvel-explorer"
  }
}
